<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>JM2008</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>greetEveryone</servlet-name>
    <servlet-class>com.jm2008.learn.servlet.GreetServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>greetEveryone</servlet-name>
    <url-pattern>/greet</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>institute</param-name>
    <param-value>InfoCampus Pvt Ltd</param-value>
  </context-param>
</web-app>